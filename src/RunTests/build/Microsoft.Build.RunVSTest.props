<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <NugetPath Condition="$(NugetPath)==''">$(NuGetPackageRoot)</NugetPath>
    <NugetPath Condition="$(NugetPath)=='' And $(NUGET_PACKAGES)!=''">$(NUGET_PACKAGES)</NugetPath>
    <SkipExecution Condition="$(SkipExecution)==''">false</SkipExecution>
    <VSTestRunnerVersion Condition="$(VSTestRunnerVersion)==''">17.8.0</VSTestRunnerVersion>
	<UseMSBuildTestInfrastructure Condition="$(UseMSBuildTestInfrastructure)==''">true</UseMSBuildTestInfrastructure>
	<VSTestTaskAssemblyFile>$(NugetPath)microsoft.testplatform.build\$(VSTestRunnerVersion)\lib\netstandard2.0\Microsoft.TestPlatform.Build.dll</VSTestTaskAssemblyFile>
	<VSTestConsolePath>$(NugetPath)microsoft.testplatform.portable\$(VSTestRunnerVersion)\tools\netcoreapp3.1\vstest.console.dll</VSTestConsolePath>
  </PropertyGroup>
</Project>